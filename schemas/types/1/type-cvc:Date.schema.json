{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://identity.com/schemas/type-cvc:Date-v1",
  "type": "object",
  "definitions": {
    "date": {
      "title": "type-cvc:Date-v1",
      "type": "object",
      "properties": {
        "day": {
          "type": "number",
          "minimum": 1,
          "maximum": 31
        },
        "month": {
          "type": "number",
          "minimum": 1,
          "maximum": 12
        },
        "year": {
          "type": "number",
          "minimum": 1900
        }
      },
      "required": ["day", "month", "year"],
      "additionalProperties": false
    }
  }
}
